{
    "name": "amethyst/time-range",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-time-range", "time-range"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "amethyst/foo": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "time-range",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\TimeRangeServiceProvider"
            ]
        }
    }
}
